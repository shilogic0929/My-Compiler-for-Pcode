{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\build\\Compiler.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\Sources",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
                "description": "为 gdb 启用整齐打印",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
            ],
            "preLaunchTask": "cmake",  //与tasks.json 的label项目同名

        }
    ]
}